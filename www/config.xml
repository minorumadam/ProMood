<?xml version='1.0' encoding='utf-8'?>
<widget id="com.promood.promoodapp" version="0.9.5" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>ProMood</name>
    <description>
   Promood App.
  </description>
    <author email="sample@promood.com" href="http://www.promood.com">
        Promood
    </author>
    <preference name="Fullscreen" value="true" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="Orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="KeyboardShrinksView" value="false" />
    <preference name="webviewbounce" value="false" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="BackupWebStorage" value="local" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    
    <plugin name="Media" value="CDVSound" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <feature name="Calendar">
        <param name="ios-package" value="Calendar" />
    </feature>

    <!-- for iOS -->
    <feature name="Insomnia">
        <param name="ios-package" value="Insomnia" />
    </feature>

    <!-- for Android -->
    <feature name="Insomnia">
        <param name="android-package" value="nl.xservices.plugins.Insomnia" />
    </feature>
    
    <content src="index.html" />
    <access origin="*" />

